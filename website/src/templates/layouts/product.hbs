<!DOCTYPE html>
<html ng-app="storeApp">
  <head>
    
  {{> global-head }}

  </head>

  <body id="home">

    {{> global-header }}

    <section class="hero-image-bg light-typo welcome-section">
        <div class="overlay-bg"></div>
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-lg-offset-4 col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2 col-xs-12 text-center wow fadeInUp">                                 

                </div>
            </div>
        </div>
    </section>

    <div class="wrapper">

      <section class="padding-top-bottom">
          <div class="container">
              <div class="row">
                  <div class="col-md-4 wow fadeInLeft">
                      <header class="section-header cta-message">
                          <center><h1>{{ name }}</h1>
                          <h3>{{ price }}</h3>
                          <h4>Height: {{ height }}</h4>
						  <h4>Diameter: {{ diameter }}</h4>
						  <h4>Wood: {{ wood }}</h4>
						  <h4>Skin: {{ skin }}</h4>
						  <h4>Finish: {{ finish }}</h4>						   
                      </center></header>
					  <center><p>{{ description }}</p>
					  <p><font size="1">{{ description2 }}</font></p></center>
					  
                  </div>
                  <div class="col-md-8 cta-message wow fadeInUp">

                       <div id="product-slides">
                          <div class="slides-container">
                              {{#each images}}
                              <img src="{{../assets}}/{{.}}" alt="" max-width="450" max-height="750">
                              {{/each}}
                          </div>
                      </div>

                      {{> body }}

                  </div>

              </div>
          </div>
      </section>

    </div>

    {{> global-footer }}

    {{> global-scripts }}

  </body>
</html>

